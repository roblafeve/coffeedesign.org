!!!
html(lang="en")
  head
    title= document.title
    meta(charset='utf-8')
    meta(content='IE=edge,chrome=1', http-equiv='X-UA-Compatible')
    meta(content='initial-scale = 1.0, user-scalable = no, width = device-width, height = device-height', name='viewport')
    meta(content='yes', name='apple-mobile-web-app-capable')
    != getBlock('meta').toHTML()
    != getBlock("styles").add(["/stylesheets/master.css"]).toHTML()
  body
    header.layout-header
      .layout-header-logo
        img.logo(src='/images/logo-coffeedesign.svg')
        h1.logo-text Coffee & Design
      .layout-header-nav
        nav
          p Navitation
    main.layout-site_content
      != content
    footer.layout-site_footer
    != getBlock('scripts').toHTML()