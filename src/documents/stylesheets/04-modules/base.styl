*
  box-sizing border-box
  margin-bottom 0
  &:first-child
    margin-top 0

html
  width 100%

body
  position absolute
  z-index 0
  margin 0
  width 100%
  background-color gray

img
  display block
  max-width 100%

a
  color inherit
  text-decoration underline
  -webkit-tap-highlight-color: rgba(0,0,0,0);